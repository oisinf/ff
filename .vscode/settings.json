{
  "files.autoSave": "off",
  "editor.formatOnSave": true,
  "eslint.run": "onType",
  "eslint.enable": true,
  "eslint.quiet": false,
  "eslint.validate": [
    "html",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
